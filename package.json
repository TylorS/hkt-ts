{
  "name": "hkt-ts",
  "version": "1.5.0",
  "description": "A Generic HKT encoding for TypeScript",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "typings": "esm/index.d.ts",
  "main:ts": "src/index.ts",
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.24",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "clipboardy": "2.3.0",
    "concurrently": "^6.5.1",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "yargs": "^17.4.1"
  },
  "scripts": {
    "build:cjs": "rm -rf cjs && tsc -p ./tsconfig.json --module commonjs --outDir cjs",
    "build:esm": "rm -rf esm && tsc -p ./tsconfig.json --outDir esm",
    "build": "concurrently npm:build:esm npm:build:cjs",
    "generate-overload": "node -r esbuild-register ./tools/overloads/cli.ts",
    "eslint": "eslint --ext .ts,.js,.json,.yaml --fix src",
    "mocha": "mocha -r esbuild-register 'src/**/*.test.ts'",
    "test": "concurrenctly npm:eslint npm:mocha",
    "preversion": "npm test",
    "postversion": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/typed-lib-starter.git"
  },
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TylorS/typed-lib-starter/issues"
  },
  "homepage": "https://github.com/TylorS/typed-lib-starter#readme",
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "ts-toolbelt": "^9.6.0"
  }
}
